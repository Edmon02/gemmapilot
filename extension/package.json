{
    "name": "gemmapilot",
    "displayName": "GemmaPilot",
    "description": "Professional AI coding assistant powered by Gemma-3n",
    "version": "0.1.0",
    "publisher": "your-publisher-name",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/gemmapilot"
    },
    "keywords": [
        "ai",
        "assistant",
        "completion",
        "chat",
        "gemma",
        "copilot"
    ],
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Machine Learning",
        "Other"
    ],
    "activationEvents": [
        "onView:gemmapilot.chat",
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "gemmapilot",
                    "title": "GemmaPilot",
                    "icon": "$(comment-discussion)"
                }
            ]
        },
        "views": {
            "gemmapilot": [
                {
                    "id": "gemmapilot.chat",
                    "name": "Chat",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(comment-discussion)"
                }
            ]
        },
        "commands": [
            {
                "command": "gemmapilot.executeCommand",
                "title": "Execute Command",
                "category": "GemmaPilot"
            },
            {
                "command": "gemmapilot.openChat",
                "title": "Open Chat",
                "category": "GemmaPilot"
            },
            {
                "command": "gemmapilot.debug",
                "title": "Debug Test",
                "category": "GemmaPilot"
            }
        ],
        "keybindings": [
            {
                "command": "gemmapilot.openChat",
                "key": "ctrl+shift+g",
                "mac": "cmd+shift+g",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/node": "^16.11.7",
        "typescript": "^4.9.3"
    }
}